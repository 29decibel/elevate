{
  "name": "stravistixdevwrapper",
  "version": "1.0.0",
  "description": "Wrap the needed tools required to cook the StravistiX Web Plugin",
  "scripts": {
    "test": "gulp specs",
    "dev:core": "gulp cleanInlineSources && tsc -w",
    "dev:webapp": "cd ./plugin/webapp/ && ng build --watch",
    "dev": "concurrently --kill-others --names \"core,webapp\" \"npm run dev:core\" \"npm run dev:webapp\"",
    "build": "rimraf dist && webpack",
    "watch": "rimraf dist && webpack -w"
  },
